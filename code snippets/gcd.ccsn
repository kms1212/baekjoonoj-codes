*/

int gcd(int a, int b) {
	return b ? gcd(b, a % b) : a;
}

long int gcdl(long int a, long int b) {
	return b ? gcdl(b, a % b) : a;
}

long long int gcdll(long long int a, long long int b) {
	return b ? gcdll(b, a % b) : a;
}