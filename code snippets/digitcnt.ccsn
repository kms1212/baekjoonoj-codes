int digitcnt(int n) {
    int r = 1;
    for (; n != 0; n /= 10) r++;
    return r;
}

int digitcntl(long int n) {
    int r = 1;
    for (; n != 0; n /= 10) r++;
    return r;
}

int digitcntll(long long int n) {
    int r = 1;
    for (; n != 0; n /= 10) r++;
    return r;
}